client.on("message", async msg => {
  let cfxy = await db.fetch(`kufur_${msg.guild.id}`);
  if (cfxy == "AÃ§Ä±k") {
    const kufur = [
      "discord.gg",
      "https//",
      ".com",
      ".xyz",
      ".net",
      ".com.tr",
      ".glitch.me",
      ".org",
      ".net",
      ".site",
      ".co"
    ];
    if (kufur.some(word => msg.content.includes(word))) {
      try {
        const dmihbar = new Discord.RichEmbed()
          .setTitle("Sunucunda " + msg.author.tag + " REKLAM YAPIYOR!")
          .setColor(0x00ae86)
          .setDescription(
            msg.author +
              "kullanÄ±cÄ±sÄ± " +
              msg.guild +
              " sunucusunda reklam yaptÄ±."
          )
          .addField("KullanÄ±cÄ±nÄ±n mesajÄ±:", "**" + msg.content + "**");

        msg.guild.owner.send(dmihbar); //CodeFENIX

        if (!msg.member.hasPermission("BAN_MEMBERS")) {
          msg.delete();

          return msg
            .reply("Reklam yapmamalÄ±sÄ±n dostum!")
            .then(msg => msg.delete(50000)); //CodeFENIX
        }
      } catch (err) {
        console.log(err);
      }
    }
  } else if (cfxy == "KapalÄ±") {
  }
});
